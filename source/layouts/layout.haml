!!!
%html
  %head
    %meta{charset: "utf-8"}/
    / Always force latest IE rendering engine or request Chrome Frame
    %meta{content: "IE=edge,chrome=1", "http-equiv" => "X-UA-Compatible"}/
    %meta{content: "width=device-width, initial-scale=1.0", name: "viewport"}/
    %meta{content: "", name: "description"}/
    %meta{content: "", name: "author"}/
    / Use title if it's in the page YAML frontmatter
    %title Ryan Snodgrass
    <link href="//netdna.bootstrapcdn.com/font-awesome/4.1.0/css/font-awesome.min.css" rel="stylesheet">
    = stylesheet_link_tag "normalize", "bootstrap", "main"
    = javascript_include_tag "Chart"
    %script{src: "/javascripts/jquery.js", type: "text/javascript"}
    %script{src: "/javascripts/bootstrap.min.js", type: "text/javascript"}
    / %script{src: "https://code.jquery.com/jquery-1.10.2.min.js"}
    / %script{src: "assets/js/bootstrap.min.js"}
    / %script{src: "assets/js/main.js"}
    %script{src: "/javascripts/masonry.pkgd.min.js"}
    %script{src: "javascripts/imagesloaded.js"}
    %script{src: "javascripts/classie.js"}
    %script{src: "javascripts/AnimOnScroll.js"}

    %script{src: "http://blacktie.co/adpacks/demoad.js"}
  %body{class: page_classes}
    = yield