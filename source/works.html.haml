/ Modal
#myModal.modal.fade{"aria-hidden" => "true", "aria-labelledby" => "myModalLabel", role: "dialog", tabindex: "-1"}
  .modal-dialog
    .modal-content
      .modal-header
        %button.close{"aria-hidden" => "true", "data-dismiss" => "modal", type: "button"} ×
        %h4#myModalLabel.modal-title contact us
      .modal-body
        .row.centered
          %p We are available 24/7, so don't hesitate to contact us.
          %p
            Somestreet Ave, 987
            %br/
            London, UK.
            %br/
            +44 8948-4343
            %br/
            hi@blacktie.co
          #mapwrap
            %iframe{frameborder: "0", height: "300", marginheight: "0", marginwidth: "0", scrolling: "no", src: "https://www.google.es/maps?t=m&ie=UTF8&ll=52.752693,22.791016&spn=67.34552,156.972656&z=2&output=embed", width: "100%"}
